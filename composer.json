{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "based/momentum-modal": "^0.2.0",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.8",
        "laravel/framework": "^10.10",
        "laravel/jetstream": "^4.2",
        "laravel/octane": "^2.3",
        "laravel/sanctum": "^3.3",
        "laravel/telescope": "^4.17",
        "laravel/tinker": "^2.8",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.32",
        "pestphp/pest-plugin-drift": "^2.5",
        "pestphp/pest-plugin-laravel": "^2.2",
        "pestphp/pest-plugin-stressless": "^2.2",
        "pestphp/pest-plugin-type-coverage": "^2.8",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-ray": "^1.33"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "format": [
            "./vendor/bin/pint",
            "npm run lint"
        ],
        "analyze": [
            "./vendor/bin/phpstan analyse --memory-limit=-1"
        ],
        "test": [
            "./vendor/bin/pest --parallel --compact --exclude-group=production"
        ],
        "test:all": [
            "./vendor/bin/pest"
        ],
        "test:profile": [
            "./vendor/bin/pest --profile"
        ],
        "test:failed": [
            "./vendor/bin/pest --retry"
        ],
        "test:stress": [
            "./vendor/bin/pest --compact --group=stress"
        ],
        "test:coverage": [
            "./vendor/bin/pest --coverage --memory-limit=-1"
        ],
        "type-coverage":  [
            "./vendor/bin/pest --type-coverage --memory-limit=-1"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
